<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SKatexMathView">
        <attr name="skmv_latex" format="string" />
        <attr name="skmv_textSize" format="dimension" />
        <attr name="skmv_textColor" format="color" />
        <attr name="skmv_background" format="reference" />
        <attr name="skmv_font" format="string" />
        <attr name="skmv_alignVertical" format="enum">
            <enum name="start" value="0" />
            <enum name="center" value="1" />
            <enum name="end" value="2" />
        </attr>
        <attr name="skmv_alignHorizontal" format="enum">
            <enum name="start" value="0" />
            <enum name="center" value="1" />
            <enum name="end" value="2" />
        </attr>
    </declare-styleable>

</resources>